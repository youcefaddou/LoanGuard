model User {
  id           Int         @id @default(autoincrement())
  email        String      @unique
  password     String      @db.VarChar(255)
  firstName    String      @db.VarChar(100)
  lastName     String      @db.VarChar(100)
  role         String      @db.VarChar(50)
  activityLogs ActivityLog[]
  userBanks    UserBank[]
  loans        Loan[]
  reports      Report[]
}